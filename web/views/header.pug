append header
    style
        include /css/header.css

    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    
    div(class="top-header")
        header
            nav(class="navbar navbar-dark navbar-expand-sm")
                div(class="navbar-nav mr-auto")
                    ul(class="navbar-nav")
                        each route in headeritems
                            - if (route.items) {
                                li(class="nav-item dropdown")
                                    a(class="btn nav-link dropdown-toggle" role="button" data-bs-toggle="dropdown" href="#") !{route.name}
                                    ul(class="dropdown-menu border border-dark")
                                        each item in route.items
                                            a(class="dropdown-item" href=`/${item.url}`) !{item.name}
                            - }
                            - else {
                                li(class="nav-item")
                                    a(class="btn nav-link" href=`/${route.url}`) !{route.name}
                            - }