	"version": "0.2.0",
	"configurations": [
		{
			"name": "EventSub",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"args": ["-debug"],
			"cwd": "${workspaceRoot}/Golang",
			"program": "${workspaceRoot}/Golang/cmd/EventSub/main.go"
		},
		{
			"name": "Firehose",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"args": ["-debug"],
			"cwd": "${workspaceRoot}/Golang",
			"program": "${workspaceRoot}/Golang/cmd/Firehose/main.go"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": ["<node_internals>/**"],
			"program": "${file}",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		}
	]
}
